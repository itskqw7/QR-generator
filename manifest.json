{
  "manifest_version": 2,
  "name": "Basic QR code generator",
  "version": "2025.1.0",
  "permissions": ["contextMenus", "activeTab", "clipboardRead"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "logo.svg"
  },
  "icons": {
    "16": "logo.svg",
    "48": "logo.svg",
    "128": "logo.svg",
    "512": "logo.svg"
  }
}
